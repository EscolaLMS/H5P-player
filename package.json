  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/html-escaper": "^3.0.0",
    "@types/jest": "^26.0.15",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^12.0.0",
    "@types/react": "^17||^18",
    "@types/react-dom": "^17||^18",
